class Tic extends Thread{
    public void run(){
    for(int i=1;i<1000;i++)
        System.out.println(Thread .currentThread().getName()+"...."+i);
    }
}

class TicketDemo2{
    public static void main(String[] args) {
        Tic t=new Tic();
        Tic q=new Tic();

        t.run();
        q.run();
        t.start();
        q.start();
    }
}
